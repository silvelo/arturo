{
  "name": "silvelo.github.io",
  "version": "2.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/silvelo.github.io/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "tag:patch": "npm version patch",
    "tag:minor": "npm version minor",
    "tag:major": "npm version major",
    "deploy:patch": "npm run tag:patch && git push && git push --tags",
    "deploy:minor": "npm run tag:minor && git push && git push --tags",
    "deploy:major": "npm run tag:major && git push && git push --tags",
    "postbuild": "copy-files-from-to"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.4",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^13.3.4",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@angular/service-worker": "~13.3.0",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-brands-svg-icons": "^6.1.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "luxon": "^2.3.2",
    "luxon-angular": "^5.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/luxon": "^2.3.1",
    "@types/node": "^12.11.1",
    "copy-files-from-to": "^3.2.2",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.6.2",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "copyFiles": [
    {
      "from": "dist/silvelo.github.io/index.html",
      "to": "dist/silvelo.github.io/404.html"
    }
  ]
}
